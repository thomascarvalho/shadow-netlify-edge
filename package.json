{
  "name": "cljs-reagent-reframe-unocss-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:css -- --watch\" \"shadow-cljs watch web\"",
    "build": "npm run build:js",
    "build:js": "shadow-cljs release --verbose web",
    "build:css": "npx unocss \"build/js/**/*.js\" \"build/js/main.js\" --out-file \"build/css/main.css\"",
    "serve": "serve build"
  },
  "devDependencies": {
    "@unocss/cli": "^0.34.0",
    "@unocss/preset-uno": "^0.34.0",
    "@unocss/preset-web-fonts": "^0.34.0",
    "@unocss/preset-wind": "^0.34.0",
    "concurrently": "^7.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "serve": "^13.0.2",
    "shadow-cljs": "^2.16.0",
    "unocss": "^0.34.0"
  },
  "dependencies": {
    "preact": "^10.5.15",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.0.1"
  }
}
